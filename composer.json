{
    "name": "ifacesoft/ice",
    "type": "library",
    "description": "Ice PHP framework",
    "keywords": ["framework"],
    "homepage": "http://iceframework.net",
    "license": "BSD ICE",
    "config": {
        "vendor-dir": "../_vendor"
    },
    "repositories": [
        {
            "type": "package",
